import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int a[]={49,42,7,20,24};
		Bubblesort(a);
	}
	public static void Bubblesort(int []a){
	    for(int i=0;i<a.length;i++){
	        for(int j=0;j<a.length-1-i;j++){
	            if(a[j]>a[j+1]){
	                int temp=a[j];
	                a[j]=a[j+1];
	                a[j+1]=temp;
	            }
	        }
	    }
	    System.out.println(Arrays.toString(a));
	}
}
